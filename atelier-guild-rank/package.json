    "hooks:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "phaser": "^3.87.0",
    "phaser3-rex-plugins": "^1.80.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "lefthook": "^2.0.15",
    "typescript": "^5.7.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.0"
  }
}
