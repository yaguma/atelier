{
  "name": "atelier-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "pnpm --filter atelier-guild-rank dev",
    "build": "pnpm --filter atelier-guild-rank build",
    "test": "pnpm --filter atelier-guild-rank test",
    "test:watch": "pnpm --filter atelier-guild-rank test:watch",
    "test:coverage": "pnpm --filter atelier-guild-rank test:coverage",
    "test:e2e": "pnpm --filter atelier-guild-rank test:e2e",
